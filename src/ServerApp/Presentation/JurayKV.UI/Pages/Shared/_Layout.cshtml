                                                </li>
                                                <li class="menu-item">
                                                    <a class="" asp-page="/Overview">Overview</a>
                                                </li>
                                                
                                            </ul>
                                        </li>

                                        <li class="menu-item">
                                            <a asp-page="/Advertise">Advertise</a>

                                        </li>
                                        <li class="menu-item">
                                            <a asp-page="/Howitworks">How it Works</a>

                                        </li>
                                        <li class="menu-item">
                                            <a asp-page="/Pricing">Pricing</a>

                                        </li>
                                        <li class="menu-item">
                                            <a href="#">Account</a>
                                            <i class="fa fa-chevron-down gen-submenu-icon"></i>

                                            <ul class="sub-menu">

                                                <li class="menu-item">
                                                    <a class="" asp-area="Auth" asp-page="/Account/Register">Register</a>
                                                </li>
                                                <li class="menu-item">
                                                    <a class="" asp-area="Auth" asp-page="/Account/Login">Login</a>
                                                </li>


                                            </ul>
                                        </li>



                                    </ul>
                                </div>
                            </div>

                            

                            <button class="navbar-toggler" type="button" data-toggle="collapse"
                                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                                    aria-expanded="false" aria-label="Toggle navigation">
                                <i class="fas fa-bars"></i>
                            </button>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header>



    @RenderBody()

